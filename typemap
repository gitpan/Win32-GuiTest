BOOL			T_IV
LONG			T_IV
HANDLE			T_IV
HWND              T_IV
HKEY			T_IV
SECURITY_INFORMATION	T_UV
DWORD			T_UV
UINT            T_UV
REGSAM			T_UV

#############################################################################
INPUT
T_UV
	$var = ($type)SvUV($arg)

#############################################################################
OUTPUT
T_UV
	sv_setuv($arg, (UV)$var);
